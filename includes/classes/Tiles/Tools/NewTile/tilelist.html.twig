<div class="new-tile__tile-list">
    {% for tile in tile.getTiles %}
        <div>
            <div>
                <span>ID:</span> {{ tile.getId() }}
            </div>
            <div>
                <span>{{ site.getString(14) }}:</span> {{ tile.getTitle() }}
            </div>
            <div>
                <span>{{ site.getString(36) }}:</span> {{ tile.getTemplatePath() }}
            </div>
            <div>
                <span>{{ site.getString(44) }}:</span> {{ tile.getClassName() }}
            </div>
        </div>
    {% endfor %}
</div>